{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@root/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@models/*": [
        "src/lib/models*"
      ]
    },
    "typeRoots": [
      "@types",
      "./src/types"
  ],
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "./dist/types/**/*.ts"
, "src/lib/common/auth/CheckLogin.tsDISABLED"  ],
  "exclude": [
    "node_modules"
  ]
}
